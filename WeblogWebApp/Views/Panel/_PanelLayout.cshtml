    @RenderSection("styles", required: false)
    <!--end::چیدمان تم ها-->

    <style>
        .follow_slider {
            color: #7b7a82;
            font-size: 14px;
            transition: left 0.2s ease;
        }

        #kt_quick_followers h3,
        #kt_quick_following h3 {
            background: #18b1aa;
            padding: 5px 15px;
            border-radius: 10px;
            color: #fff;
        }

        .follow_slider ul {
            list-style-type: none;
            padding: 0;
        }

        .follow_slider li {
            height: 60px;
            padding: 5px;
        }

            .follow_slider li:last-child {
                border-bottom: 0;
            }

            .follow_slider li .slider_item,
            .follow_slider li .slider_left {
                display: flex;
            }

            .follow_slider li .slider_item {
                justify-content: space-between;
                align-items: center;
                height: 100%;
            }

        .follow_slider img {
            width: 35px;
            border-radius: 50%;
            margin-left: 10px;
        }

        .follow_slider .item {
            text-align: left;
            font-size: 12px;
        }

        .follow_slider .item_email {
            font-weight: 300;
            margin-top: 5px;
        }

        .follow_slider .btn {
            padding: 5px 10px;
            border-radius: 25px;
            cursor: pointer;
            font-size: 12px;
            text-align: center;
        }

            .follow_slider .btn.btn_follower {
                background: #6a60c3;
                color: #fff;
            }

                .follow_slider .btn.btn_follower:hover {
                    background: #8178d1;
                }

            .follow_slider .btn.btn_follow {
                border: 1px solid #7b7a82;
            }

                .follow_slider .btn.btn_follow:hover {
                    color: #fff;
                    background: #7b7a82;
                }

        .follow_slider.active {
            left: 100%;
        }

        .settings_btn.active {
            background: #6a60c3;
        }

        .settings_btn i {
            transition: transform 0.2s ease;
        }

        .settings_btn.active i {
            transform: rotate(360deg);
        }
    </style>
</head>
