    @if (TempData["Status"] != null)
    {
        <script>
            Swal.fire(
                '@TempData["Status"]' == 'success' ? "تبریک!" : "وای!",
                '@TempData["Message"]',
                '@TempData["Status"]' == 'success' ? "success" : "error"
            )

        </script>

    }
    @if (TempData["Errors"] != null)
    {
        <script>
            jQuery(document).ready(function () {
                let deserialized = JSON.parse('@TempData["Errors"]'.replace(/&quot;/g, '"'))
                deserialized.forEach(x => {
                    let field = $(`[name=${x.Name}]`)
                    if (x.Errors.length > 0) {
                        field.addClass("is-invalid")
                        x.Errors.forEach(e => {
                            field.after(`<div class="invalid-feedback">${e}</div>`)
                        })
                    }
                    else
                        field.addClass("is-valid")
                })

            })

        </script>
    }

    @await RenderSectionAsync("scripts", required: false)


